#include <stdio.h>

int main(){
    int num;
    
    printf("Enter your rating b/w 1-10\n");
    scanf("%d",&num);
    
    if(num>=1 && num<=5)
        printf("average/n");
    else if(num>=6 && num<=7)
        printf("good/n");
    else if(num>=8 && num<=9)
        printf("very good/n");
    else if(num==10)
        printf("excellent/n");
    else
        printf("You have enter an invalid rating");
    
    
    return 0;
    }
