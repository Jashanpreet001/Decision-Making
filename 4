#include <stdio.h>

int main(){
    
    int marks;
    char grade;
    printf("Enter your marks obtained\n");
    scanf("%d",&marks);
    
    if(marks>=90)
        grade='A';
    else if(marks>=80)
        grade='B';
    else if(marks>=70)
         grade='C';
    
    else
        grade='D';
        
    printf("The grade obtained is %c\n",grade);
    return 0;
    
}
